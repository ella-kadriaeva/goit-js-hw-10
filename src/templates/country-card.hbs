{{#each this}}
<div class="card">
  <div class="card-img">
    <img src="{{flags.svg}}" alt="{{name.official}}" width="60px" height="30px">
    <p class="card-item__title">{{name.official}}</p>
  </div>
  <div class="card-body">
    
    <p class="item__text"><b>Capital</b>: {{capital}}</p>
    <p class="item__text"><b>Population</b>: {{population}}</p>
    <p class="item__text"><b>Languages</b>:
    {{#each languages}}
    <span>{{this}};</span>
    {{/each}}
  </div>
</div>
{{/each}}